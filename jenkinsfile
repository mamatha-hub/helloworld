pipeline {
    agent {
        
        docker {
            
            image 'maven:3-alpine'
            args '-v /root/.m2:/root/.m2'
            label 'jenkins_slave'
        }
    }
     stages {
        stage('Build') {
         agent { label 'jenkins_slave' }
            steps {
                sh 'mvn clean install'
            }
        }
        }
        }
